{
  "expo": {
    "name": "NoticeBoard",
    "slug": "testapk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_transparent.png",
    "splash": {
      "image": "./assets/Logo_Transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.semicolon.noticeboard"
    },
    "android": {
      "package": "com.semicolon.noticeboard",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
